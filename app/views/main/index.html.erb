<%= render 'layouts/topbar' %>

<div class="container-fluid">
	<div class="row-fluid">

		<%= render 'layouts/leftmenu' %>

		<%= render 'layouts/noscript_warning' %>
	<div id="content" class="span10">
			<!-- content starts -->
		<div>
			<ul class="breadcrumb">
				<li>
					<a href="#">主页</a> 
				</li>
			</ul>
		</div>
		<div class="sortable row-fluid">
			<a data-rel="tooltip" title="128个新增用户." class="well span3 top-block" href="#">
				<span class="icon32 icon-red icon-user"></span>
				<div>总用户数</div>
				<div>8192</div>
				<span class="notification">128</span>
			</a>

			<a data-rel="tooltip" title="16个新增订单." class="well span3 top-block" href="#">
				<span class="icon32 icon-color icon-star-on"></span>
				<div>本月日订单数</div>
				<div>111</div>
				<span class="notification green">16</span>
			</a>

			<a data-rel="tooltip" title="新增营收￥123000." class="well span3 top-block" href="#">
				<span class="icon32 icon-color icon-cart"></span>
				<div>本月营收</div>
				<div>￥3332000</div>
				<span class="notification yellow">￥123000</span>
			</a>
			
			<a data-rel="tooltip" title="52条未读用户留言." class="well span3 top-block" href="#">
				<span class="icon32 icon-color icon-envelope-closed"></span>
				<div>用户留言</div>
				<div>1500</div>
				<span class="notification red">52</span>
			</a>
		</div>
		
		<div class="row-fluid">
			<div class="box span12">
				<div class="box-header well">
					<h2><i class="icon-info-sign"></i> 订单配送情况</h2>
					<div class="box-icon">
						<a href="#" class="btn btn-minimize btn-round"><i class="icon-chevron-up"></i></a>
						<a href="#" class="btn btn-close btn-round"><i class="icon-remove"></i></a>
					</div>
				</div>
				<div class="box-content">
					<h1><small>共</small> 16 <small>个订单</small></h1>
					<p>其中 <b><big>5</big></b>  个待配送订单</p>
					<p>其中 <b><big>3</big></b>  个待发货订单</p>
					<p>其中 <b><big>8</big></b>  个已发货订单</p>
					<p class="center"></p>
					<div class="clearfix"></div>
				</div>
			</div>
		</div>
				
		<div class="row-fluid sortable">  
			<div class="box span12">
				<div class="box-header well" data-original-title>
					<h2><i class="icon-list-alt"></i> 实时流量</h2>
					<div class="box-icon">
						<a href="#" class="btn btn-minimize btn-round"><i class="icon-chevron-up"></i></a>
						<a href="#" class="btn btn-close btn-round"><i class="icon-remove"></i></a>
					</div>
				</div>
				<div class="box-content">
					<div id="realtimechart" style="height:190px;"></div>
						<p class="clearfix"></p>
						<p>时间间隔: <input id="updateInterval" type="text" value="" style="text-align: right; width:5em"> 毫秒</p>
				</div>
			</div><!--/span-->
		</div><!--/row-->  
	  

				<!-- content ends -->
		</div><!--/#content.span10-->		
        
        </div><!--/fluid-row-->
	
	<hr>

	<div class="modal hide fade" id="myModal">
		<div class="modal-header">
			<button type="button" class="close" data-dismiss="modal">×</button>
			<h3>设置</h3>
		</div>
		<div class="modal-body">
			<p>可配置属性</p>
		</div>
		<div class="modal-footer">
			<a href="#" class="btn" data-dismiss="modal">关闭</a>
			<a href="#" class="btn btn-primary">保存修改</a>
		</div>
	</div>

    <%= render 'layouts/footer' %>
	
</div><!--/.fluid-container-->


