<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>城市生活管理后台</title>
  <%= csrf_meta_tags %>
  <style type="text/css">
	body {
	  padding-bottom: 40px;
	}
	.sidebar-nav {
      padding: 9px 0;
	}
  </style>
  <%= stylesheet_link_tag    "application" %>
  <!-- The HTML5 shim, for IE6-8 support of HTML5 elements -->
  <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]--> 
<!--  <p class="notice"><%= notice %></p>
  <p class="alert"><%= alert %></p> -->
</head>
<body>

<%= yield %>

<% if defined? @jquery_1_7_2 %>
  <%= javascript_include_tag "jquery-1.7.2.min" %>
<% else %>
  <%= javascript_include_tag "jquery-1.8.0.min" %>
<% end %>

<%= javascript_include_tag "application" %>
<%= javascript_include_tag "jquery.jstree" %>
<!-- attribute-set init js -->
<% if defined? @init_attribute_set_js %>
<%= javascript_include_tag "init/attribute-set" %> 
<% end %>

<!-- products_list init js -->
<% if defined? @products_index_js %>
<%= javascript_include_tag "init/products_index" %> 
<% end %>

<!-- new-product init js -->
<% if defined? @init_new_product_js %>
<%= javascript_include_tag "init/new-product" %> 
<% end %>

<% if defined? @init_new_attribute_js %>
<%= javascript_include_tag "init/new-attribute" %>
<% end %>

<% if defined? @init_image_management %>
  <%= javascript_include_tag "elFinderSupportVer1" %>
  <%= javascript_include_tag "init/init_image_management" %>
<% end %>

<% if defined? @init_new_flashsales %>
<%= javascript_include_tag "init/new-flashsales" %>
<% end %>

<% if defined? @init_flashsales_products_selector %>
<%= javascript_include_tag "init/flashsales-products-selector" %>
<% end %>

</body>
</html>